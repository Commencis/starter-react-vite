{
  "name": "@commencis/starter-react-vite",
  "version": "2.8.0",
  "build": "0",
  "description": "Commencis Starter - React Vite",
  "author": "Commencis WEB Team",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=22.20"
  },
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint:check:types": "tsc --noEmit",
    "lint:check:js": "eslint .",
    "lint:fix:js": "eslint . --fix",
    "lint:check:css": "stylelint **/*.{css,scss}",
    "lint:fix:css": "stylelint **/*.{css,scss} --fix",
    "lint:check:format": "prettier \"**/*.{json,md}\" --check",
    "lint:fix:format": "prettier \"**/*.{json,md}\" --write",
    "lint": "pnpm run \"/^lint:check:.*/\"",
    "lint:fix": "pnpm run \"/^lint:fix:.*/\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:review": "pnpm run lint && pnpm run test",
    "generate:worker": "pnpm exec msw init ./public --save",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@reduxjs/toolkit": "2.9.0",
    "axios": "1.12.2",
    "clsx": "2.1.1",
    "i18next": "25.5.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.63.0",
    "react-i18next": "16.0.0",
    "react-redux": "9.2.0",
    "react-router": "7.9.3",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@commencis/commitlint-config": "1.2.0",
    "@commencis/eslint-config": "2.3.0",
    "@commencis/eslint-plugin": "1.6.0",
    "@commencis/lint-staged-config": "1.2.0",
    "@commencis/prettier-config": "2.0.0",
    "@commencis/stylelint-config": "2.4.0",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@storybook/react-vite": "9.1.10",
    "@svitejs/changesets-changelog-github-compact": "1.2.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.18.8",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vitest/coverage-v8": "3.2.4",
    "commitlint": "20.1.0",
    "eslint": "9.36.0",
    "husky": "9.1.7",
    "jsdom": "27.0.0",
    "lint-staged": "16.2.3",
    "msw": "2.11.3",
    "prettier": "3.6.2",
    "sass-embedded": "1.93.2",
    "storybook": "9.1.10",
    "stylelint": "16.25.0",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-svgr": "4.5.0",
    "vitest": "3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
